{
  "name": "storj.js",
  "version": "0.0.0",
  "description": "Storj.io browser library",
  "main": "index.js",
  "directories": {},
  "scripts": {
    "build": "browserify lib/index.js > storj.es6.js # && babel storj.es6.js > storj.es5.js",
    "posttest": "npm run linter",
    "test": "npm run test-node && npm run test-browser",
    "test-node": "nyc --reporter=text --reporter=html tape test/**/*.js",
    "test-browser": "testling",
    "linter": "eslint --fix .",
    "make-docs": "rm -rf ./jsdoc && ./node_modules/.bin/jsdoc lib -r -R README.md -c .jsdoc.json --verbose -d ./jsdoc",
    "node-test": "node test/**/*.js"
  },
  "browserify": {
    "transform": [
      "envify"
    ]
  },
  "testling": {
    "files": [
      "lib/browser.js",
      "test/**/*.js"
    ]
  },
  "author": "Chris Pollard",
  "contributors": [
    {
      "name": "retrohacker",
      "url": "https://github.com/retrohacker"
    }
  ],
  "license": "LGPL",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "browserify": "^13.1.1",
    "envify": "^4.0.0",
    "eslint": "^3.14.0",
    "ink-docstrap": "bookchin/docstrap",
    "jsdoc": "^3.4.3",
    "nyc": "^10.0.0",
    "proxyquire": "^1.7.10",
    "tape": "^4.6.3",
    "testling": "^1.7.1"
  },
  "dependencies": {
    "filereader-stream": "^1.0.0",
    "from2": "^2.3.0",
    "local-web-server": "^1.2.6",
    "memory-blob-store": "^1.0.0",
    "memory-chunk-store": "^1.2.0",
    "render-media": "^2.9.1",
    "setimmediate": "^1.0.5",
    "storj-lib": "github:retrohacker/core#68752abc5398bcf465d4ab0776329d65644ad2f2",
    "stream-to-blob": "^1.0.0",
    "stream-to-blob-url": "^2.1.0"
  }
}
